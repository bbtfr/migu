<div class="focus swiper-wrapper">
  <% for (i=0; i<data.length; i++) { %>
    <% o = data[i] %>
    <div class="swiper-slide">
      <% if (o.link) { %><a href="<%= o.link %>"><% } %>
      <img src="<%= o.image %>"<%= vmsTag(0, i) %>>
      <% if (o.link) { %></a><% } %>
    </div>
  <% } %>
</div>
