<div class="focus swiper-wrapper">
  <% for o in @data: %>
    <div class="swiper-slide">
      <% if o.link: %>
        <a href="<%= o.link %>">
      <% end %>
      <img src="<%= o.image %>">
      <% if o.link: %>
        </a>
      <% end %>
    </div>
  <% end %>
</div>
