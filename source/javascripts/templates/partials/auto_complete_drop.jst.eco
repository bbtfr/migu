<h2>歌曲</h2>
  <% for music in @musics: %>
    <a href="#search/<%= encodeURI(music) %>"><p><%= music %></p></a>
  <% end %>
<h2>歌手</h2>
  <% for artist in @artists: %>
    <a href="#search/<%= encodeURI(artist) %>"><p><%= artist %></p></a>
  <% end %>
<h2>专辑</h2>
  <% for album in @albums: %>
    <a href="#search/<%= encodeURI(album) %>"><p><%= album %></p></a>
  <% end %>
